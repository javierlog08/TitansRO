function	script	KafraPluginPayInstance	{
	set .@reward,getarg(0);
	getpartymember getarg(1),0;
	set .@count, $@partymembercount;
	
	if(.@count >= 1) {
		copyarray .@name$[0], $@partymembername$[0], $@partymembercount;
		// list the party member names to pay it
		for (set .@i,0; .@i < .@count; set .@i, .@i+1) {
			charcommand "#cash \""+.@name$[.@i]+"\" "+.@reward;
		}
		announce "Hey! El equipo  \""+strcharinfo(1)+"\" acaba de derrotar al "+getarg(2)+" !!!..... Todos ganaron "+.@reward+" Kafra Points!",bc_blue|bc_all;
	} else {
		announce "Hey! \""+strcharinfo(0)+"\" acaba de derrotar al "+getarg(2)+" !!!..... Gano "+.@reward+" Kafra Points!",bc_blue|bc_all;
		charcommand "#cash \""+strcharinfo(0)+"\" "+.@reward;
	}
	
}